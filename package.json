{
  "name": "todo-cdd-example",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.1",
    "@mui/material": "^5.0.1",
    "msw": "^0.35.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.0.0",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@cypress/react": "^5.10.0",
    "@cypress/webpack-dev-server": "^1.6.0",
    "@jackfranklin/test-data-bot": "^1.3.0",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addon-storyshots": "^6.3.8",
    "@storybook/node-logger": "^6.3.8",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.8",
    "@testing-library/cypress": "^8.0.1",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.0.6",
    "@types/jest": "^27.0.2",
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "cypress": "^8.4.1",
    "eslint-plugin-cypress": "^2.11.2",
    "husky": "^7.0.2",
    "jest-date-mock": "^1.0.8",
    "lint-staged": "^11.0.0",
    "loki": "^0.28.1",
    "prettier": "^2.2.1",
    "react-scripts": "4.0.3",
    "reg-cli": "^0.17.0",
    "start-server-and-test": "^1.11.5",
    "stylelint": "^13.8.0",
    "stylelint-config-xo-space": "^0.15.1",
    "typescript": "^4.1.2"
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "format": "prettier --write",
    "postinstall": "msw init public; is-ci || husky install",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:e2e": "BROWSER=none start-test 3000 'cypress run'",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public --quiet"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=6",
    "yarn": ">=1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "stylelint": {
    "extends": [
      "stylelint-config-xo-space"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "yarn format",
      "eslint --fix"
    ],
    "*.{css,scss}": [
      "yarn format",
      "stylelint --fix"
    ],
    "*.{json,md}": [
      "yarn format"
    ]
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      }
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
