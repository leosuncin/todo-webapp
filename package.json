{
  "name": "todo-cdd-example",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.6.1",
    "@material-ui/icons": "^4.5.1",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.1",
    "typescript": "3.7.5"
  },
  "devDependencies": {
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-docs": "^5.2.6",
    "@storybook/addon-knobs": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addon-viewport": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/preset-create-react-app": "^1.3.0",
    "@storybook/react": "^5.2.6",
    "@suncin/stylelint-config-scss": "^1.0.0",
    "@testing-library/cypress": "^5.0.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^8.0.4",
    "@types/jest": "25.1.3",
    "@types/node": "12.12.19",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "cypress": "^3.6.1",
    "eslint-plugin-cypress": "^2.7.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.2",
    "prettier": "^1.19.1",
    "start-server-and-test": "^1.10.6",
    "stylelint": "^13.2.0"
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "format": "prettier --write",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:e2e": "BROWSER=none start-test 3000 'cypress run'",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=6",
    "yarn": ">=1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "stylelint": {
    "extends": [
      "@suncin/stylelint-config-scss"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test --watchAll=false"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "yarn format",
      "eslint --fix",
      "git add -u"
    ],
    "*.{css,scss}": [
      "yarn format",
      "stylelint --fix",
      "git add -u"
    ],
    "*.{json,md}": [
      "yarn format",
      "git add -u"
    ]
  }
}
